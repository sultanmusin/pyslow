<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<config xmlns="http://www.w3schools.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" created="17.02.2014" id="default" xsi:noNamespaceSchemaLocation="PsdSlowControlConfig.xsd" xsi:schemaLocation="PsdSlowControlConfig.xsd">
<global>
        <flags>
<!--
            <onlineModules> 1 </onlineModules>
-->
            <queryDelay unit="sec">10</queryDelay>
            <verbose> 1 </verbose>
            <useVoiceNotifications> 0 </useVoiceNotifications>
            <voiceServer> na61daqt </voiceServer>
            <monitorHV> 0 </monitorHV>
            <refTemp> 25 </refTemp>
            <tempSlope> 60 </tempSlope>
        </flags>
        <connection>
            <hvsys id='north' timeout="0.035">
                    <rate unit="baud">57600</rate>
                    <port>192.168.0.101</port>
                    <dataBits>8</dataBits>
                    <stopBits>1</stopBits>
                    <parity>0</parity>                
            </hvsys>

        </connection>
    </global>

</config>
